upstream wiki_upstream
{
    server ${WIKI_INTERNAL_NAME} fail_timeout=3s max_fails=0;
}

server {
    include       /etc/nginx/generated/ssl.conf;

    listen        443 ssl http2;

    server_name   ${WIKI_URL};

    location / {
        include /etc/nginx/common_location.conf;
        proxy_pass  http://wiki_upstream;
    }
}

# vi:syntax=nginx
